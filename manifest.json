{
  "manifest_version": 2,

  "name": "DSR Converter and Mailer",
  "description": "My first Chrome extension.",
  "version": "1.0",
  "author": "Keval",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "My custom google Extension"
  },
  "permissions": [
    "activeTab",
    "storage",
    "https://api.sendgrid.com/*"
  ],
  "background": {
    "page": "popup.html"
  },
  "icons": { 
    "16": "icon.png",
    "48": "icon.png",
   "128": "icon.png" 
  }
}